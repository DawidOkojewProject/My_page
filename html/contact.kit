    <div class="top">
       <form action="mail.php" method="post" class="contact-form">
      <h3>Formularz kontaktowy</h3>
          <div class="contact-box">
            <label for="name">Imię i nazwisko:</label>
            <input type="text" id="name" name="name" required placeholder="Wpisz swoje imię">
          </div>
          <div class="contact-box">
            <label for="email">Email:</label>
            <input type="email" id="email" name="email" required placeholder="Podaj email">
          </div>
          <div class="contact-box">
            <label for="msg">Wiadomość:</label>
            <textarea name="msg" id="msg" required placeholder="Wpisz treść wiadomości"></textarea>
          </div>
      
         <div class="buttons">
            <button  type="reset" class="button">Wyczyść</button>
            <button type="submit"  class="button">Wyślij</button>
         </div>
      
        </form>
